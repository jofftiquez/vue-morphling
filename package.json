  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-export-default-from": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.5",

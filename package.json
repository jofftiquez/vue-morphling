{
  "name": "vue-morphling",
  "version": "1.1.2",
  "description": "A collection of VueJs filters.",
  "main": "dist/index.js",
  "semistandard": {
    "parser": "babel-eslint",
    "ignore": [
      "webpack.cofig.js",
      "dist",
      "src/directives/email"
    ],
    "globals": [
